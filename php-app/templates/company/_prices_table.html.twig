<div style="height:250px;overflow-y:scroll;display:block;">
    <table class="table">
        <thead>
            <tr>
                <th>Date</th>
                <th>Open</th>
                <th>High</th>
                <th>Low</th>
                <th>Close</th>
                <th>Volume</th>
            </tr>
        </thead>
        <tbody>
        {% for price in prices %}
            {% if price.open is defined and price.close is defined %}
            <tr>
                <td>{{ price.date|date("m/d/Y", false) }}</td>
                <td>{{ price.open }}</td>
                <td>{{ price.high }}</td>
                <td>{{ price.low }}</td>
                <td>{{ price.close }}</td>
                <td>{{ price.volume }}</td>
            </tr>
            {% endif %}
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>